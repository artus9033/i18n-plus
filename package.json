{
	"name": "i18n-plus",
	"version": "2.0.6",
	"description": "Node.js package providing i18n with variable interpolation & conjugation of words with respect to quantifiers, supporting all languages' conjugation rules.",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "mocha --require esm",
		"genDocs": "jsdoc -c ./.jsdoc.json",
		"install": "npm run build"
	},
	"homepage": "https://artus9033.github.io/i18n-plus/",
	"author": "artus9033",
	"license": "MIT",
	"devDependencies": {
		"better-docs": "^2.3.2",
		"esm": "^3.2.25",
		"jsdoc": "^3.6.6",
		"minami": "^1.2.3",
		"mocha": "^8.1.3"
	},
	"repository": {
		"url": "https://github.com/artus9033/i18n-plus"
	},
	"dependencies": {
		"@types/lodash": "^4.14.161",
		"@types/node": "^14.11.2",
		"lodash": "^4.17.20",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3"
	},
	"keywords": [
		"intl",
		"i18n",
		"internationalization",
		"locale",
		"interpolation",
		"format",
		"formatting",
		"translate",
		"translation"
	]
}
